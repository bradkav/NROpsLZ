    ";", "\[IndentingNewLine]", 
    RowBox[{"Yvals", "=", "\[IndentingNewLine]", 
     RowBox[{"If", " ", "[", 
      RowBox[{
       RowBox[{"FileExistsQ", "[", "fname", "]"}], ",", "\[IndentingNewLine]", 
