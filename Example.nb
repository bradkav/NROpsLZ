Cell[TextData[{
 "The DM-nucleon SI Cross Section as a function ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", "SI"], TraditionalForm]]],
 " is given by:\n",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[Sigma]", "p", "SI"], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    RowBox[{"(", 
     SubscriptBox["\[Lambda]", "SI"], ")"}], "2"], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["\[Mu]", "\[Chi]N", "2"], TraditionalForm]]],
 " / \[Pi]\t\[DoubleRightArrow]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Lambda]", "SI"], "(", 
    RowBox[{
     SubsuperscriptBox["\[Sigma]", "p", "SI"], " ", ",", " ", 
     SubscriptBox["m", "\[Chi]"]}], ")"}], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   SqrtBox[
    RowBox[{"\[Pi]", " ", 
     RowBox[{
      SubsuperscriptBox["\[Sigma]", "p", "SI"], " ", "/", " ", 
      SubsuperscriptBox["\[Mu]", "\[Chi]N", "2"]}]}]], TraditionalForm]]],
 "\nwhere\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Mu]", "\[Chi]N"], "=", " ", 
    RowBox[{
     SubscriptBox["m", "\[Chi]"], 
     RowBox[{
      SubscriptBox["m", "N"], "/", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["m", "\[Chi]"], "+", 
        SubscriptBox["m", "N"]}], ")"}]}]}]}], TraditionalForm]]],
 " is the DM-nucleon reduced mass"
}], "Text",
