            RowBox[{"10", "^", "4"}], "]"}], ",", " ", "0.1"}], "}"}]}], 
        "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Return", "[", 
     RowBox[{"Yvals", "[", 
      RowBox[{"[", 
       RowBox[{"All", ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", " ", 
          RowBox[{"1", "+", "ind"}]}], "}"}]}], "]"}], "]"}], "]"}]}], 
